#include <stdint.h>
#include <stm32f7.h>
#include "stm32_gpio.h"

int main(void)
{
	GPIO_Handle_t GpioLedC11;
	GpioLedC11.pGPIOx = GPIOC;
	GpioLedC11.GPIO_PinConfig.GPIO_PinNumber = GPIO_PIN_NO_11;
	GpioLedC11.GPIO_PinConfig.GPIO_PinMode = GPIO_MODE_OUT;
	GpioLedC11.GPIO_PinConfig.GPIO_PinSpeed = GPIO_SPEED_FAST;
	GpioLedC11.GPIO_PinConfig.GPIO_PinOPType = GPIO_OP_TYPE_PP;
	GpioLedC11.GPIO_PinConfig.GPIO_PinPuPdControl = GPIO_NO_PUPD;

	GPIO_Init(&GpioLedC11);

	while(1)
	{
	    GPIO_ToggleOutputPin(GPIOC, GPIO_PIN_NO_11);
	    delay();
	}

	return 0;
}
